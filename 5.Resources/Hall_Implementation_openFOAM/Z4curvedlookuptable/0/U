FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (400 0 0);

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    
    cyclic1
    {
        type            cyclic;
    }
    
    cyclic2
    {
        type            cyclic;
    }
    
    outlet
    {
        type            zeroGradient;
	}
	
    inlet
    {
        type			fixedValue;	
	    value			uniform (400 0 0);         
    }
}


